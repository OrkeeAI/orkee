{
  "name": "@orkee/tasks",
  "version": "0.0.1",
  "description": "Task management provider abstraction for Orkee - supports multiple task sources",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest"
  },
  "keywords": [
    "tasks",
    "kanban",
    "project-management"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.33",
    "@ai-sdk/openai": "^2.0.53",
    "@ai-sdk/react": "^2.0.76",
    "@ai-sdk/ui-utils": "^1.2.11",
    "@radix-ui/react-dialog": "^1.1.15",
    "@tanstack/react-query": "^5.51.11",
    "ai": "^5.0.76",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "eventemitter3": "^5.0.1",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.1.12",
    "zod-to-json-schema": "^3.24.6"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "lucide-react": "^0.542.0",
    "react": "^18.2.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  },
  "peerDependencies": {
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "lucide-react": "^0.542.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
