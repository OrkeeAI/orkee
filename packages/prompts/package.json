{
  "name": "@orkee/prompts",
  "version": "0.1.0",
  "description": "Centralized AI prompt management for Orkee",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./prd/*": "./prd/*.json",
    "./research/*": "./research/*.json",
    "./system/*": "./system/*.json"
  },
  "files": [
    "dist",
    "prd",
    "research",
    "system",
    "schema.json",
    "README.md"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsc -p tsconfig.json --watch",
    "lint": "eslint src --ext .ts",
    "test": "vitest"
  },
  "keywords": [
    "orkee",
    "ai",
    "prompts",
    "llm"
  ],
  "author": "Orkee",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.3.3",
    "vitest": "^1.0.0"
  },
  "dependencies": {}
}
