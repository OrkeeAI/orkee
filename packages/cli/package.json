{
  "name": "orkee",
  "version": "0.0.5",
  "description": "Orkee - AI agent orchestration platform with CLI, TUI, and dashboard",
  "keywords": [
    "ai",
    "agents",
    "orchestration",
    "cli",
    "dashboard"
  ],
  "homepage": "https://github.com/OrkeeAI/orkee",
  "repository": {
    "type": "git",
    "url": "https://github.com/OrkeeAI/orkee.git"
  },
  "license": "MIT",
  "author": "Orkee Team",
  "bin": {
    "orkee": "./scripts/install.js"
  },
  "files": [
    "scripts/"
  ],
  "scripts": {
    "sync-version": "node scripts/sync-version.js",
    "prepublishOnly": "npm run sync-version",
    "postinstall": "node scripts/install.js",
    "prebuild": "npm run sync-version",
    "build": "cargo build --release --features cloud",
    "dev": "cargo run --features cloud --bin orkee -- dashboard",
    "test": "cargo test --features cloud"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "tar-stream": "^3.1.0"
  }
}
