{
  "$schema": "https://turbo.build/schema.v2.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        "dist/**",
        "target/**"
      ]
    },
    "dev": {
      "persistent": true,
      "cache": false
    },
    "lint": {},
    "test": {
      "outputs": ["coverage/**"]
    }
  }
}